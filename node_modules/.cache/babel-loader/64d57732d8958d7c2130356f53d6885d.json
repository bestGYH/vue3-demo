{"ast":null,"code":"import { reactive, watch } from 'vue';\nexport default {\n  name: 'App',\n  components: {},\n\n  setup() {\n    const person = reactive({\n      name: \"张三\",\n      age: 32\n    });\n    watch(person.value, (newVal, oldVal) => {\n      console.log(\"person变化了\", newVal, oldVal);\n    });\n    return {\n      person\n    };\n  }\n\n};","map":{"version":3,"mappings":"AAWA,SAAOA,QAAP,EAAiBC,KAAjB,QAA8B,KAA9B;AACA,eAAe;EACbC,IAAI,EAAE,KADO;EAEbC,UAAU,EAAE,EAFC;;EAIbC,KAAK,GAAE;IACL,MAAMC,MAAK,GAAIL,QAAQ,CAAC;MACtBE,IAAI,EAAC,IADiB;MAEtBI,GAAG,EAAC;IAFkB,CAAD,CAAvB;IAIAL,KAAK,CAACI,MAAM,CAACE,KAAR,EAAc,CAACC,MAAD,EAAQC,MAAR,KAAiB;MAClCC,OAAO,CAACC,GAAR,CAAY,WAAZ,EAAwBH,MAAxB,EAA+BC,MAA/B;IACD,CAFI,CAAL;IAGA,OAAO;MACLJ;IADK,CAAP;EAGD;;AAfY,CAAf","names":["reactive","watch","name","components","setup","person","age","value","newVal","oldVal","console","log"],"sourceRoot":"","sources":["/Users/gaoyanhui/Study/VUE3/vue3-demo/src/App.vue"],"sourcesContent":["<template>\n  <button @click=\"person.name+='~'\">改变名字</button>\n <p> {{person.name}}</p>\n  <button @click=\"person.age+=1\">改变年龄</button>\n  <p>\n    {{person.age}}\n  </p>\n\n</template>\n\n<script>\nimport{reactive, watch } from 'vue'\nexport default {\n  name: 'App',\n  components: {\n  },\n  setup(){\n    const person = reactive({\n      name:\"张三\",\n      age:32\n    })\n    watch(person.value,(newVal,oldVal)=>{\n      console.log(\"person变化了\",newVal,oldVal)\n    })\n    return {\n      person\n    }\n  },\n}\n</script>\n\n<style>\n#app {\n  font-family: Avenir, Helvetica, Arial, sans-serif;\n  -webkit-font-smoothing: antialiased;\n  -moz-osx-font-smoothing: grayscale;\n  text-align: center;\n  color: #2c3e50;\n  margin-top: 60px;\n}\n</style>\n"]},"metadata":{},"sourceType":"module"}